  <Application.Views>
    <Ribbon>
      <Ribbon.ApplicationMenu>
        <ApplicationMenu CommandName="Application">
          <ApplicationMenu.RecentItems>
            <RecentItems CommandName="RecentItems" MaxCount="20" EnablePinning="false" />
          </ApplicationMenu.RecentItems>
          <MenuGroup Class="StandardItems">
            <Button CommandName="Open" />
            <Button CommandName="Save" />
          </MenuGroup>
          <MenuGroup Class="StandardItems">
            <Button CommandName="Options" />
            <Button CommandName="About" />
            <Button CommandName="Exit" />
          </MenuGroup>
        </ApplicationMenu>
      </Ribbon.ApplicationMenu>
      <Ribbon.HelpButton>
        <HelpButton CommandName="Help" />
      </Ribbon.HelpButton>
      <Ribbon.QuickAccessToolbar>
        <QuickAccessToolbar CommandName="Qat">
          <QuickAccessToolbar.ApplicationDefaults>
            <Button CommandName="Open" ApplicationDefaults.IsChecked="true" />
            <Button CommandName="Save" ApplicationDefaults.IsChecked="true" />
          </QuickAccessToolbar.ApplicationDefaults>
        </QuickAccessToolbar>
      </Ribbon.QuickAccessToolbar>
      <Ribbon.Tabs>
        <Tab CommandName="Home">
          <Tab.ScalingPolicy>
            <ScalingPolicy>
              <ScalingPolicy.IdealSizes>
                <Scale Group="Clipboard" Size="Medium" />
                <Scale Group="Editing" Size="Large" />
              </ScalingPolicy.IdealSizes>
              <Scale Group="Clipboard" Size="Small" />
              <Scale Group="Views" Size="Large" />
            </ScalingPolicy>
          </Tab.ScalingPolicy>
          <Group CommandName="Clipboard" SizeDefinition="FourButtons">
            <SplitButton CommandName="PasteSplit">
              <SplitButton.ButtonItem>
                <Button CommandName="Paste" />
              </SplitButton.ButtonItem>
              <Button CommandName="Paste" />
              <Button CommandName="PasteHex" />
            </SplitButton>
            <Button CommandName="Cut" />
            <Button CommandName="Copy" />
            <Button CommandName="CopyHex" />
          </Group>
          <Group CommandName="Editing">
            <SizeDefinition>
              <GroupSizeDefinition Size="Large">
                <ControlSizeDefinition ImageSize="Large" />
                <ControlSizeDefinition />
                <ControlSizeDefinition />
                <ControlSizeDefinition />
                <ColumnBreak />
                <ControlSizeDefinition IsImageVisible="false" />
              </GroupSizeDefinition>
            </SizeDefinition>
            <Button CommandName="Find" />
            <Button CommandName="FindNext" />
            <Button CommandName="FindPrevious" />
            <Button CommandName="GoTo" />
            <Button CommandName="SelectAll" />
          </Group>
          <Group CommandName="Views">
            <SizeDefinition>
              <GroupSizeDefinition Size="Large">
                <ControlSizeDefinition IsImageVisible="false" />
                <ControlSizeDefinition IsImageVisible="false" />
              </GroupSizeDefinition>
            </SizeDefinition>
            <ComboBox CommandName="Encoding" IsEditable="false" />
            <CheckBox CommandName="Bits" />
          </Group>
        </Tab>
      </Ribbon.Tabs>
    </Ribbon>
    <ContextPopup>
      <ContextPopup.ContextMenus>
        <ContextMenu Name="Context">
          <MenuGroup>
            <Button CommandName="CutContext" />
            <Button CommandName="CopyContext" />
            <Button CommandName="PasteContext" />
          </MenuGroup>
          <MenuGroup>
            <Button CommandName="SelectAllContext" />
          </MenuGroup>
        </ContextMenu>
      </ContextPopup.ContextMenus>
      <ContextPopup.ContextMaps>
        <ContextMap CommandName="ContextMenus" ContextMenu="Context" />
      </ContextPopup.ContextMaps>
    </ContextPopup>
  </Application.Views>
</Application>
